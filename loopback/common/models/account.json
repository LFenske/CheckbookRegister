  "options": {
    "validateUpsert": true
  },
  "properties": {
    "custId": {
      "type": "string",
      "required": true
    },
    "acctname": {
      "type": "string",
      "required": true
    },
    "clearcode": {
      "type": "string",
      "default": "01"
    },
    "nextline": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "count"
    }
  ],
  "methods": {}
}
